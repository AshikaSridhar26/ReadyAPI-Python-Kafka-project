          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// Thread number (1..N)
def threadNum = ctx.getThreadNum() + 1

// Global counter (unique across all threads)
def counter = vars.getObject(&quot;GLOBAL_COUNTER&quot;)
if (counter == null) {
    counter = new java.util.concurrent.atomic.AtomicInteger(0)
    vars.putObject(&quot;GLOBAL_COUNTER&quot;, counter)
}

def seq = counter.incrementAndGet()

// RUN_ID passed from ReadyAPI via -J
def runId = props.get(&quot;RUN_ID&quot;, &quot;local-run&quot;)

// Generate values
vars.put(&quot;customerId&quot;, &quot;CUST-&quot; + threadNum + &quot;-&quot; + seq)
vars.put(&quot;customerName&quot;, &quot;LoadUser-&quot; + threadNum)
vars.put(&quot;correlationId&quot;, runId + &quot;-&quot; + java.util.UUID.randomUUID().toString())
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
